{
  "name": "tripr-app-frontend",
  "version": "0.0.0",
  "engines": {
    "npm": ">=11.0.0",
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "npm run api:generate:ifneeded && vite",
    "build": "npm run api:generate:ifneeded && vite build",
    "preview": "vite preview",
    "test": "npm run api:generate:ifneeded && vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "npm run api:generate:ifneeded && vitest run --coverage",
    "api:generate": "openapi-generator-cli generate",
    "api:generate:ifneeded": "node -e \"const fs=require('fs'); if(!fs.existsSync('src/app/core/api/generated')) { require('child_process').execSync('npm run api:generate', {stdio:'inherit'}); }\"",
    "api:clean": "del-cli src/app/core/api/generated"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/platform-browser-dynamic": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@auth0/angular-jwt": "^5.2.0",
    "@fontsource/open-sans": "^5.2.7",
    "@jsverse/transloco": "^8.2.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.3.0",
    "@analogjs/vitest-angular": "^2.3.0",
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.5",
    "@openapitools/openapi-generator-cli": "^2.29.0",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitest/ui": "^4.0.18",
    "del-cli": "^7.0.0",
    "jsdom": "^28.1.0",
    "puppeteer": "^24.37.3",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
